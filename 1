import java.io.*;
class operators
{
 public static void main(String[] args)
    {
  int a=5,b=10,c=5;
System.out.println(a==b);
System.out.println(a==c);
System.out.println(a>b);
System.out.println(a<b);
System.out.println(a>=c);
System.out.println(a<=b);
System.out.println(a!=b);
}
}
output:
D:\>javac operators.java

D:\>java operators
false
true
false
true
true
true
true
